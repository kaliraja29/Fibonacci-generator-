def generate_fibonacci(n):
    fib_sequence = []
    a, b = 0, 1
    for _ in range(n):
        fib_sequence.append(a)
        a, b = b, a + b
    return fib_sequence

# Example: Generate first 10 Fibonacci numbers
num_terms = 10
fibonacci_numbers = generate_fibonacci(num_terms)
print("Fibonacci sequence:")
print(fibonacci_numbers)
